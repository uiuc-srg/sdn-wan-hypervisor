node n1 {
    type OVS
    model OVS
    network-config {
	hostname n1
	!
    }
    canvas c1
    iconcoords {413.0 165.0}
    labelcoords {413.0 197.0}
    interface-peer {eth0 n2}
    interface-peer {eth1 n3}
    interface-peer {eth2 n4}
    interface-peer {eth3 n5}
}

node n2 {
    type router
    model host
    network-config {
	hostname n2
	!
	interface eth0
	 ip address 10.0.0.10/24
	 ipv6 address 2001:0::10/64
	!
    }
    canvas c1
    iconcoords {183.0 101.0}
    labelcoords {183.0 133.0}
    interface-peer {eth0 n1}
}

node n3 {
    type router
    model host
    network-config {
	hostname n3
	!
	interface eth0
	 ip address 10.0.0.11/24
	 ipv6 address 2001:1::10/64
	!
    }
    canvas c1
    iconcoords {272.0 335.0}
    labelcoords {272.0 367.0}
    interface-peer {eth0 n1}
}

node n4 {
    type router
    model host
    network-config {
	hostname n4
	!
	interface eth0
	 ip address 10.0.0.12/24
	 ipv6 address 2001:2::10/64
	!
    }
    canvas c1
    iconcoords {503.0 108.0}
    labelcoords {503.0 140.0}
    interface-peer {eth0 n1}
}

node n5 {
    type router
